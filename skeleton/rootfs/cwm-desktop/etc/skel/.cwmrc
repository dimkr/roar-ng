# ~/.cwmrc: cwm configuration

# windows to ignore
ignore stalonetray

# the default font
fontname "Terminus:size=10"

# default applications
command term urxvt
command lock slock

# applications menu
command Vifm "urxvt -title Vifm -e vifm"
command Links "urxvt -title Links -e links"
command Sylpheed sylpheed
command Snownews "urxvt -title Snownews -e snownews"
command Irssi "urxvt -title Irssi -e irssi"
command rTorrent "urxvt -title rTorrent -e rtorrent"
command Calcurse "urxvt -title Calcurse -e calcurse"
command TuDu "urxvt -title TuDu -e tudu"
command AlsaMixer "urxvt -title AlsaMixer -e alsamixer"
command cmus "urxvt -title cmus -e cmus"
command UnNetHack "urxvt -title UnNetHack -e unnethack"
command Geany geany
command Vim "urxvt -title Vim -e vim"
command htop "urxvt -title htop -e htop"
